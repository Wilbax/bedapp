WindowManager:
    LoadingScreen:
    UserTypeScreen:


<LoadingScreen>:
    name: "loadingS"
    MDBoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        MDLabel:
            text: "first screen"
            font_size:32

        MDRoundFlatIconButton:
            text: "next screen"
            font_size: 32
            on_press: root.current = "userTypeS"


<UserTypeScreen>:
    name: "userTypeS"
    MDBoxLayout:
        orientation: "vertical"
        size: root.width, root.height

        MDLabel:
            text: "first screen"
            font_size:32

        MDRoundFlatIconButton:
            text: "Go back"
            font_size: 32
            on_press:root.current = "LoadingS"
